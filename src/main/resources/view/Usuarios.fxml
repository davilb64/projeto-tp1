<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="controller.UsuariosController" spacing="10" alignment="CENTER">
    <Label fx:id="tituloLabel" text="Gestão de Usuários"/>
    <Button fx:id="cadastrarButton" text="Cadastrar Novo Usuário" onAction="#cadastrarUsuario"/>
    <Button fx:id="listarButton" text="Listar Usuários" onAction="#listarUsuarios"/>
</VBox>


